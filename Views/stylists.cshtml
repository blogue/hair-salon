<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Hair Salon</h1>
    @if(Model.Count == 0)
    {
      <h1>No Stylists Available</h1>
    }
    else
    {
      <ul>
      @foreach(var stylist in Model)
      {
        <li><a href="/stylist/@stylist.GetId()">@stylist.GetName()</a></li>
      }
      </ul>
    }
    <a href="/stylists/add"><button type="button" name="button">Add Stylists</button></a>
    <a href="/"><button type="button" name="button">Home</button></a>
    <a href="/stylists/deleted"><button type="button" name="button">Delete All</button></a>
  </body>
</html>
